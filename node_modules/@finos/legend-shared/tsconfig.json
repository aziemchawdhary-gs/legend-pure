{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "webworker",
      "scripthost"
    ],
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "target": "esnext",
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "importsNotUsedAsValues": "error",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./lib",
    "tsBuildInfoFile": "./build/prod.tsbuildinfo",
    "rootDir": "./src"
  },
  "references": [
    {
      "path": "./tsconfig.package.json"
    }
  ],
  "files": [
    "./src/CommonUtils.ts",
    "./src/index.ts",
    "./src/application/AbstractPluginManager.ts",
    "./src/application/ActionState.ts",
    "./src/application/BrowserUtils.ts",
    "./src/application/HashUtils.ts",
    "./src/application/RandomizerUtils.ts",
    "./src/application/SerializationUtils.ts",
    "./src/application/StopWatch.ts",
    "./src/application/TestUtils.ts",
    "./src/communication/EventNotifierService.ts",
    "./src/communication/IframeEventNotifierPlugin.ts",
    "./src/communication/TelemetryService.ts",
    "./src/data-structure/Pair.ts",
    "./src/data-structure/Stack.ts",
    "./src/error/AssertionUtils.ts",
    "./src/error/ErrorUtils.ts",
    "./src/format/FormatterUtils.ts",
    "./src/format/ValidatorUtils.ts",
    "./src/log/Logger.ts",
    "./src/log/WebConsole.ts",
    "./src/markdown/MarkdownUtils.ts",
    "./src/network/AbstractServerClient.ts",
    "./src/network/NetworkUtils.ts",
    "./src/network/TracerService.ts"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.json"
  ],
  "exclude": [
    "src/**/__tests__/**/*.ts",
    "src/**/__tests__/**/*.tsx",
    "src/**/__mocks__/**/*.ts",
    "src/**/__mocks__/**/*.tsx"
  ]
}